<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:mixinTypes="[cq:Console]"
          jcr:primaryType="cq:Widget"
          jcr:title="Wordbee AEM Translation"
          sling:redirect="{Boolean}false"
          sling:resourceType="cq/ui/components/widget"
          sling:vanityOrder="{Long}300"
          sling:vanityPath="/translation"
          consoleDescription="Translate AEM pages"
          consoleTitle="Beebox Translation Management"
          historyLink="/bin/wcmcommand?cmd=open&amp;path=${resource.path}"
          historyResourceTypes="[cq:Page]"
          historyShow="{Boolean}true"
          historyTitle="Recently used pages"
          iconClass="tladmin"
          id="cq-tladmin"
          jsLibs="[cq.wcm.admin]"
          noSearch="{Boolean}false"
          tabTitle="Beebox Translation Management"
          treeAutoExpandMax="100"
          xtype="tladmin">
    <grid jcr:primaryType="nt:unstructured">
        <assets
                jcr:primaryType="nt:unstructured"
                pageSize="15"
                defaultSortable="true"
                pageText="Assets {0} - {1} of {2}"
                pathRegex="\\/content\\/dam(/.*)?"
                storeProxySuffix=".assets.json"
                storeReaderFields="[index,title,label,path,pageLanguage,languageNiceName,status,translatedTo,translatedFrom]">
            <columns jcr:primaryType="cq:WidgetCollection">
                <numberer
                        jcr:primaryType="nt:unstructured"
                        usePredefined="numberer"
                        xtype="gridcolumn"/>
                <title
                        jcr:primaryType="nt:unstructured"
                        hidden="{Boolean}true"
                        usePredefined="title"
                        xssProtect="true"
                        xtype="gridcolumn"/>
                <name
                        jcr:primaryType="nt:unstructured"
                        usePredefined="name"
                        xtype="gridcolumn"/>
                <path
                        jcr:primaryType="nt:unstructured"
                        usePredefined="path"
                        xtype="gridcolumn"/>
                <pageLanguage
                        jcr:primaryType="nt:unstructured"
                        usePredefined="pageLanguage"
                        xtype="gridcolumn"/>
                <status
                        jcr:primaryType="nt:unstructured"
                        usePredefined="status"
                        xtype="gridcolumn"/>
                <translatedTo
                        jcr:primaryType="nt:unstructured"
                        usePredefined="translatedTo"
                        xtype="gridcolumn"/>
                <translatedFrom
                        jcr:primaryType="nt:unstructured"
                        usePredefined="translatedFrom"
                        xtype="gridcolumn"/>
                <basketActions
                        jcr:primaryType="nt:unstructured"
                        usePredefined="basketActions"
                        xtype="gridcolumn"/>
            </columns>
        </assets>
    </grid>
    <actions jcr:primaryType="nt:unstructured">
        <separator2
                jcr:primaryType="nt:unstructured"
                value="-"
                xtype="separator"/>
        <separator3
                jcr:primaryType="nt:unstructured"
                value="->"
                xtype="separator"/>
        <separator4
                jcr:primaryType="nt:unstructured"
                value="-"
                xtype="separator"/>
        <tools
                jcr:primaryType="nt:unstructured"
                cls="cq-siteadmin-tools"
                context="[toolbar]"
                iconCls="cq-siteadmin-tools-icon"
                text="Settings">
            <menu jcr:primaryType="nt:unstructured">
                <download
                        jcr:primaryType="nt:unstructured"
                        cls="cq-siteadmin-findreplace"
                        handler="CQ.wcm.TLAdmin.beeboxDialogListener"
                        text="Wordbee Beebox configuration"/>
                <completedJobs
                        jcr:primaryType="nt:unstructured"
                        cls="cq-siteadmin-rollout"
                        handler="CQ.wcm.TLAdmin.languageMappingDialogListener"
                        id="cq-tladmin-language-mapping-button"
                        text="Beebox Target Language Mapping"/>
                <aboutBeebox
                        jcr:primaryType="nt:unstructured"
                        cls="cq-siteadmin-rollout"
                        handler="CQ.wcm.TLAdmin.aboutUsListener"
                        id="cq-tladmin-about-us-button"
                        text="About"/>
            </menu>
        </tools>
        <separator5
                jcr:primaryType="nt:unstructured"
                value="->"
                xtype="separator"/>
        <separator6
                jcr:primaryType="nt:unstructured"
                value="-"
                xtype="separator"/>
        <basket
                jcr:primaryType="nt:unstructured"
                cls="cq-siteadmin-help"
                handler="CQ.wcm.TLAdmin.beeboxBasketHandler"
                id="cq-tladmin-basket-button"
                text="(0)"
                tooltip="Show Basket"/>
        <separator1
                jcr:primaryType="nt:unstructured"
                value="-"
                xtype="separator"/>
        <jobs
                jcr:primaryType="nt:unstructured"
                cls="cq-siteadmin-rollout"
                handler="CQ.wcm.TLAdmin.quotationItemHandler"
                id="cq-tladmin-quotation-button"
                text="My Jobs"/>
    </actions>
    <tags jcr:primaryType="nt:unstructured">
        <emulateIE8
                jcr:primaryType="nt:unstructured"
                content="IE=8"
                http-equiv="X-UA-Compatible"
                tagName="meta"/>
        <icon1
                jcr:primaryType="nt:unstructured"
                href="/libs/wcm/core/content/siteadmin.ico"
                rel="shortcut icon"
                tagName="link"
                type="image/vnd.microsoft.icon"/>
        <icon2
                jcr:primaryType="nt:unstructured"
                href="/libs/wcm/core/content/siteadmin.ico"
                rel="icon"
                tagName="link"
                type="image/vnd.microsoft.icon"/>
    </tags>
</jcr:root>